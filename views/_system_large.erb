<!-- graphs for cluster's system status -->
<div class="row">&nbsp;</div>
<div class="row">
    <div class="col-sm-3 col-xs-6">
        <div class="panel panel-default panel-clickable" onclick="update_large_graph('<%= h ganglia.report_moab_nodes.size(800,250).png %>',<%= count %>)">
            <div class="panel-heading"><h4 class="panel-title">Nodes</h4></div>
            <div class="panel-body">
              <img class='img-responsive center-block updateable' src='<%= h ganglia.small.report_moab_nodes.png %>' alt='Graph' onclick="update_large_graph('<%= h ganglia.report_moab_nodes.size(800,250).png %>',<%= count %>)">
            </div>
        </div>
    </div>
    <div class="col-sm-3 col-xs-6">
        <div class="panel panel-default panel-clickable" onclick="update_large_graph('<%= h ganglia.report_moab_jobs.size(800,250).png %>',<%= count %>)">
            <div class="panel-heading"><h4 class="panel-title">Jobs</h4></div>
            <div class="panel-body">
              <img class='img-responsive center-block updateable' src='<%= h ganglia.small.report_moab_jobs.png %>' alt="Graph" onclick="update_large_graph('<%= h ganglia.report_moab_jobs.size(800,250).png %>',<%= count %>)">
            </div>
        </div>
    </div>
    <div class="col-sm-3 col-xs-6">
        <div class="panel panel-default panel-clickable" onclick="update_large_graph('<%= h ganglia.report_load.size(800,250).png %>',<%= count %>)">
            <div class="panel-heading"><h4 class="panel-title">Load</h4></div>
            <div class="panel-body">
                <img class='img-responsive center-block updateable' src='<%= h ganglia.small.report_load.png %>' alt="Graph" onclick="update_large_graph('<%= h ganglia.report_load.size(800,250).png %>',<%= count %>)">
            </div>
        </div>
    </div>
    <div class="col-sm-3 col-xs-6">
        <div class="panel panel-default panel-clickable" onclick="update_large_graph('<%= h ganglia.report_network.size(800,250).png %>',<%= count %>)">
            <div class="panel-heading"><h4 class="panel-title">Network</h4></div>
            <div class="panel-body">
                <img class='img-responsive center-block updateable' src='<%= h ganglia.small.report_network.png %>' alt='Graph' onclick="update_large_graph('<%= h ganglia.report_network.size(800,250).png %>',<%= count %>)">
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        <div class=" panel panel-default">
            <div class="panel-body" id="large-status">
                <img src='<%= eval("ganglia.report_moab_nodes.size(800,250).png") %>' class='img-responsive center-block updateable' id='large-image<%= count %>' >
            </div>
        </div>
    </div>
</div>