<div>

    <script type="text/javascript">
        var hour = "<%= render partial: 'system_large', locals: {ganglia: @ganglia.hour} %>";
        var two_hours = "<%= render partial: 'system_large', locals: {ganglia: @ganglia.two_hours} %>";
        var four_hours = "<%= render partial: 'system_large', locals: {ganglia: @ganglia.four_hours} %>";
        var day = "<%= render partial: 'system_large', locals: {ganglia: @ganglia.day} %>";
        var week = "<%= render partial: 'system_large', locals: {ganglia: @ganglia.week} %>";
        var month = "<%= render partial: 'system_large', locals: {ganglia: @ganglia.month} %>";
        var year = "<%= render partial: 'system_large', locals: {ganglia: @ganglia.year} %>";
    </script>

    <!-- Nav tabs -->
    <div id="time-tabs">
        <ul class="nav nav-tabs nav-justified" id="hour-tabs" role="tablist">
            <li role="presentation" class="active"><a href="#hour" aria-controls="hour" role="tab" data-toggle="tab">Hour</a></li>
            <li role="presentation"><a href="#twohour" aria-controls="twohour" role="tab" data-toggle="tab">2 Hours</a></li>
            <li role="presentation"><a href="#fourhour" aria-controls="fourhour" role="tab" data-toggle="tab">4 Hours</a></li>
            <li role="presentation"><a href="#day" aria-controls="day" role="tab" data-toggle="tab">Day</a></li>
            <li role="presentation"><a href="#week" aria-controls="week" role="tab" data-toggle="tab">Week</a></li>
            <li role="presentation"><a href="#month" aria-controls="month" role="tab" data-toggle="tab">Month</a></li>
            <li role="presentation"><a href="#year" aria-controls="year" role="tab" data-toggle="tab">Year</a></li>
        </ul>
        <div class="tab-content">

        </div>
    </div>

    <!-- Tab panes -->
    <div class="tab-content">
        <div role="tabpanel" class="tab-pane active" id="hour">
            <%= render partial: 'system_large', locals: {ganglia: @ganglia.hour} %>
        </div>
        <div role="tabpanel" class="tab-pane" id="twohour">
            <%= render partial: 'system_large', locals: {ganglia: @ganglia.two_hours} %>
        </div>
        <div role="tabpanel" class="tab-pane" id="fourhour">
            <%= render partial: 'system_large', locals: {ganglia: @ganglia.four_hours} %>
        </div>
        <div role="tabpanel" class="tab-pane" id="day">
            <%= render partial: 'system_large', locals: {ganglia: @ganglia.day} %>
        </div>
        <div role="tabpanel" class="tab-pane" id="week">
            <%= render partial: 'system_large', locals: {ganglia: @ganglia.week} %>
        </div>
        <div role="tabpanel" class="tab-pane" id="month">
            <%= render partial: 'system_large', locals: {ganglia: @ganglia.month} %>
        </div>
        <div role="tabpanel" class="tab-pane" id="year">
            <%= render partial: 'system_large', locals: {ganglia: @ganglia.year} %>
        </div>
    </div>

</div>

<div class="col-md-12 well">
    DEBUG<br>
    User: <%= ENV['USER'] %><br>
    Path: <%= ENV['PWD'] %><br>
    Server YML: <%= OSC_Servers.to_s %><br>
    Ganglia IMG: <%= @ganglia.img %>
</div>